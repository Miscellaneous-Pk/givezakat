    <style media="screen">
    /* http://meyerweb.com/eric/tools/css/reset/
       v2.0 | 20110126
       License: none (public domain)
    */

    html, body, div, span, applet, object, iframe,
    h1, h2, h3, h4, h5, h6, p, blockquote, pre,
    a, abbr, acronym, address, big, cite, code,
    del, dfn, em, img, ins, kbd, q, s, samp,
    small, strike, strong, sub, sup, tt, var,
    b, u, i, center,
    dl, dt, dd, ol, ul, li,
    fieldset, form, label, legend,
    table, caption, tbody, tfoot, thead, tr, th, td,
    article, aside, canvas, details, embed,
    figure, figcaption, footer, header, hgroup,
    menu, nav, output, ruby, section, summary,
    time, mark, audio, video {
    	margin: 0;
    	padding: 0;
    	border: 0;
    	font-size: 100%;
    	font: inherit;
    	vertical-align: baseline;
    }
    /* HTML5 display-role reset for older browsers */
    article, aside, details, figcaption, figure,
    footer, header, hgroup, menu, nav, section {
    	display: block;
    }
    body {
    	line-height: 1;
    }
    ol, ul {
    	list-style: none;
    }
    blockquote, q {
    	quotes: none;
    }
    blockquote:before, blockquote:after,
    q:before, q:after {
    	content: '';
    	content: none;
    }
    table {
    	border-collapse: collapse;
    	border-spacing: 0;
    }

    body{
      font-family: 'Montserrat', sans-serif;
      padding-bottom: 30vh;
    }

    .container, .logo-area {
      width: 100vw;
      max-width: 1251px;
      padding-left: 1.5rem;
      margin: 0 auto;
      /* border: 1px solid red; */
      display: flex;
      box-sizing: border-box;
    }

    .content {
      width: 70%;
      max-width: 933px;
      position: relative;
      box-sizing: border-box;
    }

    .sticky-nav {
      width: 30%;
      box-sizing: border-box;
      order: 1;
    }

    h2{
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 20px;
      line-height: 24px;

      /* Green 1 */
      color: #27AE60;
      margin-top: 45px;
      margin-bottom: 22px;
    }

    .disc-block {
      display: grid;
      grid-template-rows: 1fr 1fr 1fr;
      grid-template-columns: 1fr 1fr;
      /* Gray 6 */
      background: #F2F2F2;
      /* Gray 6 */
      border: 1px solid #F2F2F2;
      box-sizing: border-box;
      padding: 14px 34px;

      font-family: Montserrat;
      font-style: normal;
      font-size: 15px;
      line-height: 27px;

      /* Gray 1 */
      color: #333333;

    }

    .disc-block>div:nth-last-child(2) {
      grid-column: 2/3;
      grid-row: 1/3;
    }

    .disc-block>div:nth-last-child(2) div {
      border: 1px dashed darkseagreen;
      border-radius: 5px;
    }

    .disc-block>div:nth-last-child(1) {
      grid-column: 2/3;
      grid-row: 3/4;
    }



    .disc-block>div {
      display: grid;
      grid-template-columns: 27px 1fr;
      grid-template-rows: auto;
    }

    .disc-block>div svg {
      grid-column: 1/2;
      opacity: 0.9;
    }

    .disc-block>div div{
      grid-column: 2/3;
      padding: 0 10px;
      margin-top: -1px;
    }

    .disc-block > * {
      margin: 10px 0;
    }

    .para-h {
      font-weight: bold;
      color: #656d66;
    }

    .para-d {
      font-weight: normal;
      color: #545453;
    }

    .title-bar {
      display: flex;
      align-items: center;
      justify-content: space-between;
      /* Gray 6 */
      color: #F2F2F2;
      font-weight: bold;
      /* Green 1 */
      background: #27AE60;
      cursor: pointer;
    }

    .title-bar:not(:nth-child(1)) {
      margin-top: 10px;
    }

    .title-bar > div {
      display: flex;
      align-items: center;
    }

    .title-bar > div > p, .lower-bar > p, .lower-bar > ol {
      display: inline-block;
      padding-left: 11px;
      padding-right: 11px;
      line-height: 22px;
      margin-left: initial !important;
    }

    .title-bar, .lower-bar {
      padding: 0 34px;
      min-height: 56px;
    }

    .lower-bar {
      display: flex;
      align-items: center;
      background: #F2F2F2;
      border: 1px solid #E0E0E0;
      font-size: 15px;
      box-sizing: border-box;
      color: #333333;
    }

    .lower-bar > *:nth-last-child(1) {
      margin-left: auto;
    }

    .lower-bar.topic, .lower-bar.important {
      align-items: start;
      padding-top: 17px;
      padding-bottom: 14px;
    }

    .lower-bar.video {
      /* font-weight: bold; */
      /* opacity: 0.8; */
    }

    .video > p:nth-last-child(1) {
      padding: 2px 11px;
      border: 1px solid #27AE60;
      background: transparent;
      color: #27AE60;
      font-size: 15px;
      font-weight: bold;
    }

    .subj-card {
      width: 100%;
      max-width: 243px;
      height: auto;
      background: #F2F2F2;
      color: #27AE60;
      opacity: 0.8;

      margin-bottom: 20px;
      border-radius: 2px;
      padding: 16px;
      box-sizing: border-box;
      border: 1px solid transparent;
    }

    .subj-card:hover {
      opacity: 1;
      cursor: pointer;
      border:1px dashed #27AE60;
    }

    .subj-card.true {
      background: #27AE60;
      color: #F2F2F2;
      opacity: 1;
    }

    .subj-card > p:nth-child(1) {
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 33px;
      line-height: 178.4%;
    }

    .subj-card > p:nth-child(2) {
      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 15px;
      line-height: 27px;
    }

    .sticky-nav > * {
      max-width: 243px;
      margin-right: auto;
      margin-left: auto;
    }

    .sticky-nav > *:not(:nth-child(1)) {
      margin-bottom: 10px;
    }

    .partition {
      height: 10px;
    }

    .topics_list {
      line-height: 30px;
      padding-left: 11px;
      margin-top: 0px;
      width: fit-content;
      margin-left: 0 !important;
    }

    .topics_list > *:not(:nth-child(1)) {
      display: inline-flex;
      background: yellowgreen;
      padding: 0px 12px;
      margin: 2px;
      color: black;
      font-weight: bold;
      font-size: 12px;
    }

    .topics_list > *:nth-child(1) {
      display: block;
      background: none;
      color: black;
      padding: 0;
      margin-bottom: 6px;
      font-weight: normal;
    }

    .imp_list, .questions_list {
      line-height: 30px;
      padding-left: 9px;
      margin-top: -3px;
      width: fit-content;
      margin-left: 0 !important;
    }

    .imp_list > *:not(:nth-child(1)) {
      display: inline-flex;
      background: lightcoral;
      padding: 0px 12px;
      margin: 2px;
      color: white;
      font-weight: bold;
      font-size: 12px;
    }

    .imp_list > *:nth-child(1), .questions_list > *:nth-child(1) {
      display: block;
      background: none;
      color: black;
      padding: 0;
      padding-left: 3px;
      margin-bottom: 6px;
      font-weight: normal;
    }

    .questions_list > *:not(:nth-child(1)) {
      align-content: normal;
      display: inline-flex;
      padding: 2px 12px;
      margin: 3px;
      color: white;
      font-weight: normal;
      font-size: 15px;
      background: #27AE60;
      width: fit-content;
      display: block;
    }

    button, a {
      padding: 2px 11px;
      border: 1px solid #27AE60;
      background: transparent;
      color: #27AE60;
      font-size: 15px;
      font-weight: bold;
      text-decoration: none;
      font-family: system-ui;
      white-space: nowrap;
    }

    button:hover, a:hover {
      background: #27AE60;
      cursor: pointer;
      color: white;
    }

    .overlay {
      height: 100vh;
      width: 100vw;
      position: fixed;
      top: 0;
      display: none;
    }

    .overlay.show {
      display: block;
      background: black;
      z-index: 100;
    }

    .overlay > iframe {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%,-50%);
    }

    .overlay > button {
      position: absolute;
      top: 1rem;
      left: 1rem;
      z-index: 100;
    }

    .overlay > h6 {
      position: absolute;
      top: 1.3rem;
      left: 6rem;
      z-index: 100;
      color: #27AE60;
      font-weight: bold;
    }

    .overlay > img {
      object-fit: contain;
      width: 100%;
      max-width: 1080px;
      max-height: 90vh;
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%,-50%);
    }

    .overlay > p {
      position: absolute;
      top: 50%;
      left: 50%;
      transform: translate(-50%);
      color: #27AE60;
      max-width: 600px;
      width: 100vw;
    }

    .overlay > svg {
      position: absolute;
      top: 50%;
      cursor: pointer;
      opacity: 0.5;
      background: black;
    }

    .overlay > svg:nth-last-child(1) {
      left: 1rem;
    }

    .overlay > svg:nth-last-child(2) {
      right: 1rem;
    }

    .title-bar.not-collapsed .closed {
      display: none;
    }

    .title-bar:not(.not-collapsed) .opened {
      display: none;
    }

    .collapse_all {
      margin-left: auto;
      position: absolute;
      right: 0;
      top: -46px;
      margin-right: 34px;
    }

    .content-block svg {
      min-width: fit-content;
      width: auto;
    }

    .d-none {
      display: none;
    }

    .error {
      text-align: center;
      font-weight: bold;
      color: red;
    }

    .section {
      background: lightcoral !important;
    }

    .my-sticker {
      width: 100%;
      max-width: 243px;
      height: auto;
      background: #F2F2F2;
      color: #27AE60;
      opacity: 1;

      margin-bottom: 20px;
      border-radius: 2px;
      padding: 16px;
      box-sizing: border-box;
      border: 1px solid transparent;
    }

    .my-sticker > p {
      font-family: Montserrat;
      font-style: normal;
      font-weight: normal;
      font-size: 15px;
      line-height: 27px;
      color: #333333;
    }

    .my-sticker > button {
      padding: 6px 8px;
      border: 1px solid transparent;
      font-family: Montserrat;
      color: #F2F2F2;
      background-color: #27AE60;
      margin: 10px 0px;
      font-weight: normal;
    }

    .my-sticker > a {
      text-decoration: none;
      background-color: #27AE60;
      color: #F2F2F2;
      cursor: pointer;
      font-family: Montserrat;
      font-size: 15px;
      text-align: center;
      margin: 0;
      padding: 10px 8px;
      max-width: 100%;
      white-space: nowrap;
      overflow: hidden;
      text-overflow: ellipsis;
      font-weight: bold;
      -webkit-font-smoothing: antialiased;
      border-radius: 2px;
      border: 1px solid transparent;
      margin-top: 10px;
      -moz-osx-font-smoothing:grayscale;
    }

    .my-sticker > a:not(.d-none) {
      display: inline-block;
    }

    small {
      font-size: 12px;
    }

    .my-sticker > textarea {
      width: 100%;
      border: 1px solid #27AE60;
      margin-top: 10px;
      font-family: Montserrat;
      font-size: 15px;
      line-height: 27px;
      padding: 10px;
      box-sizing: border-box;
      color: darkgreen;
      font-weight: bold;
    }

    .my-sticker > textarea::placeholder {
      opacity: 0.5;
    }

    .mobile {
      display: none;
    }

    .desktop {
      display: block;
    }

    b {
      font-weight: bold;
    }

    .logo-area {
      display: flex;
      height: 44px;
      align-items: center;
      padding: 0 31px 0 20px;
      cursor: pointer;
      position: fixed;
      background: white;
      z-index: 100;
      left: 50%;
      transform: translate(-50%);
    }

    .logo-area > p:hover {
      color: black;
    }

    .logo-area > p {
      padding-left: 10px;

      /* Quran Daily */

      font-family: Montserrat;
      font-style: normal;
      font-weight: bold;
      font-size: 14px;
      line-height: 157.9%;
      /* identical to box height, or 32px */
      display: flex;
      align-items: center;

      /* Gray 2 */
      color: #4F4F4F;
    }

    .logo-area > p:nth-child(2) {
      opacity: 0;
    }

    .logo-area > p:nth-child(3) {
      margin-left: auto;
    }

    .modal {
      position: fixed;
      left: 50%;
      top: 50%;
      transform: translate(-50%, -50%);
      background: #26ae60;
      border: 1px solid lavenderblush;
      padding: 13px 18px;
      color: white;
    }

    .modal > p {
      line-height: 176%;
    }

    .modal > button {
      background: whitesmoke;
      padding: 8px 13px;
      margin-top: 8px;
    }

    @media (max-width: 769px) {

      .modal {
        width: 80%;
        box-sizing: border-box;
      }

      .logo-area {
        padding: 0 24px;
      }


      .mobile {
        display: block;
      }

      .sticky-nav.mobile {
        margin-top: 10px;
      }

      .desktop {
        display: none;
      }

      h2 {
        margin-top: 10px;
      }

      .container {
        flex-wrap: wrap;
        padding-left: 0rem;
      }

      .sticky-nav {
        width: 100%;
        order: 0;
        padding: 0 10px;
      }

      .sticky-nav > * {
        max-width: 100%;
      }

      .content {
        padding: 0 10px;
        width: 100%;
      }

      .disc-block {
        grid-template-columns: 1fr;
        grid-template-rows: auto;
        padding: 0px;
        width: 100%;
      }

      .disc-block>div:nth-last-child(2), .disc-block>div:nth-last-child(1) {
        grid-column: auto;
        grid-row: auto;
      }

      .title-bar, .lower-bar {
        padding: 0px 2px;
      }

      .collapse_all {
        margin-right: 0px;
      }

      .overlay>p {
        text-align: center;
      }

      .subj-card > p:nth-child(1) {
        display: none;
      }

      .title-bar > div > p, .lower-bar > p, .lower-bar > ol {
        padding-left: 4px;
        padding-right: 4px;
        word-break: break-word;
        margin-left: initial !important;
      }
    }

    </style>
  </head>
