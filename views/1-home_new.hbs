    <div class="container">

    {{#each data}}

    {{#if (match 'featured_blog' type)}}
      {{>featured_blog}}
    {{/if}}

    {{#if (match 'person' type)}}
      {{>person}}
    {{/if}}

    {{#if (match 'blog' type)}}
      {{>blog}}
    {{/if}}

    {{#if (match 'course' type)}}
      {{>course}}
    {{/if}}

    {{#if (match 'signin' type)}}
      {{>signin session=../session}}
    {{/if}}

    {{#if (match 'meetup' type)}}
      {{>meetup}}
    {{/if}}

    {{#if (match 'subscribe' type)}}
      {{>subscribe}}
    {{/if}}

    {{#if (match 'video' type)}}
      {{>video}}
    {{/if}}

    {{#if (match 'footer' type)}}
      {{>footer}}
    {{/if}}

    {{/each}}

    </div>

    <script type="text/javascript">
      $(document).on('mouseenter mouseleave','.card',function() {
        $(this).toggleClass('hover');
      })

      function goto(msg) {
        window.location.href = msg;
      }

    </script>
  </body>
</html>
